<h1>Login</h1>

<% if flash[:message]%>
<%= flash[:message] %>
<% end %>
<br>
<br>
<%= form_for :user do |f| %>
<%= f.label :email %><br>
<%= f.email_field :email, required:true %><br>
<%= f.label :password %><br>
<%= f.password_field :password, required:true %><br>
<%= f.submit "Log In" %>
<% end %>