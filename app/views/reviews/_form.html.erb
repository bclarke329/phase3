

<%= form_with(model: @review, url: product_reviews_path(@product)) do |f| %>
<%= f.label :title %><br>
<%= f.text_field :title %><br>
<%= f.label :rating %><br>
<%= f.number_field :rating%><br>
<%= f.label :review %><br>
<%= f.text_area :review %><br>
<%= f.submit "Submit Review" %><br>
<% end %>
